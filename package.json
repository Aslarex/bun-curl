  "files": ["dist"],
  "types": "./dist/index.d.ts",
  "optionalDependencies": {
    "redis": "^4.7.0",
    "prettier": "^3.5.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^22.13.5",
    "bun-types": "^1.2.3",
    "typescript": "^5.7.3"
